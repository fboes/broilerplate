# editorconfig.org

root = true

[*]
charset = utf-8
indent_style = tab
insert_final_newline = true
trim_trailing_whitespace = true
end_of_line = lf
tab_width = 4

[*.md]
trim_trailing_whitespace = false

# generated files

[{package.json,bower.json,**/*.css}]
indent_style = space
indent_size = 2

[{**/*.css}]
insert_final_newline = false
