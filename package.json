{
  "name": "broilerplate",
  "description": "A boilerplate with SASS & JS.",
  "version": "3.0.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:fboes/broilerplate.git"
  },
  "devDependencies": {
    "@ronilaukkarinen/gulp-stylelint": "^14.0.5",
    "autoprefixer": "^10.4.0",
    "browser-sync": "^2.27.7",
    "eslint": "^8.5.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^6.0.0",
    "gulp-mocha": "^8.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.0.0",
    "gulp-uglify": "^3.0.2",
    "postcss": "^8.4.5",
    "postcss-scss": "^4.0.2",
    "sass": "^1.45.1",
    "stylelint": "^14.2.0",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-recommended-scss": "^5.0.2"
  },
  "config": {
    "develop": false
  },
  "directories": {
    "doc": "docs",
    "lib": "src",
    "htdocs": "htdocs",
    "template": "htdocs",
    "sass": "src/sass",
    "css": "htdocs/css",
    "js_src": "src/js-src",
    "js": "htdocs/js",
    "images": "htdocs/images",
    "tools": "tools"
  },
  "scripts": {
    "build": "gulp",
    "start": "gulp watch",
    "deploy-live": "tools/deploy.sh live",
    "test": "npm run test-js && npm run test-css && npm outdated",
    "test-js": "npx eslint **/*.js --fix",
    "test-css": "npx stylelint **/*.scss --fix"
  },
  "author": "Frank BoÃ«s",
  "license": "MIT",
  "private": false,
  "browserslist": [
    "last 2 versions",
    "> 2%",
    "ie 9",
    "ie 10",
    "ie 11"
  ]
}
